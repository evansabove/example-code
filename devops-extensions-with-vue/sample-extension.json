{
    "manifestVersion": 1,
    "id": "sample-extension",
    "publisher": "andyevansdev",
    "version": "1.0.17",
    "name": "Sample Extension",
    "description": "{your-extension-description}",
    "public": false,
    "categories": ["Azure Repos"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "sample-extension",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "Sample Extension",
                "uri": "index.html"
            }
        }
    ],
    "files": [
        {
            "path": "sample-extension-app/.output/public",
            "addressable": true,
            "packagePath": "/"
        }
    ],
    "scopes": [
        "vso.code" 
    ]
}